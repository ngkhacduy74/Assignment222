                       document.addEventListener("DOMContentLoaded", function () {
            let roleFilter = document.getElementById("roleFilter");
            let userTableRows = document.querySelectorAll("#userTable tbody tr");

            roleFilter.addEventListener("change", function () {
                let selectedRole = roleFilter.value;

                userTableRows.forEach(row => {
                    let userRole = row.getAttribute("data-role");

                    if (selectedRole === "all" || userRole === selectedRole) {
                        row.style.display = "";
                    } else {
                        row.style.display = "none";
                    }
                });
            });
        });//lọc theo role
                document.addEventListener("DOMContentLoaded", function () {
            let userTable = document.querySelector("#userTable tbody");

            // Bắt sự kiện khi click vào badge trạng thái (Active/Deactive)
            userTable.addEventListener("click", function (event) {
                if (event.target.classList.contains("status-toggle")) {
                    let statusBadge = event.target;
                    let row = statusBadge.closest("tr");
                    let userId = row.getAttribute("data-userid");
                    let roleId = row.getAttribute("data-role");

                    if (roleId == "3") {
                        alert("Tài khoản Admin không thể bị Deactive!");
                        return;
                    }

                    // Toggle trạng thái
                    if (statusBadge.classList.contains("badge-outline-success")) {
                        statusBadge.classList.remove("badge-outline-success");
                        statusBadge.classList.add("badge-outline-danger");
                        statusBadge.textContent = "Deactive";

                        // Thêm nút xóa nếu là Deactive và không phải Admin
                        let deleteBtn = row.querySelector(".delete-btn");
                        if (!deleteBtn) {
                            let deleteCell = row.insertCell(-1);
                            deleteCell.innerHTML = `<button class="btn btn-danger btn-sm delete-btn">Delete</button>`;
                        }
                    } else {
                        statusBadge.classList.remove("badge-outline-danger");
                        statusBadge.classList.add("badge-outline-success");
                        statusBadge.textContent = "Active";

                        // Xóa nút delete nếu Active
                        let deleteBtn = row.querySelector(".delete-btn");
                        if (deleteBtn) {
                            deleteBtn.closest("td").remove();
                        }
                    }
                }
            });

            // Bắt sự kiện xóa tài khoản
            userTable.addEventListener("click", function (event) {
                if (event.target.classList.contains("delete-btn")) {
                    let row = event.target.closest("tr");
                    let userId = row.getAttribute("data-userid");

                    if (confirm("Bạn có chắc chắn muốn xóa tài khoản này?")) {
                        row.remove();
                        alert("Tài khoản đã bị xóa.");
                    }
                }
            });
        });



    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/microsoft-signalr/7.0.5/signalr.min.js"></script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Kết nối đến Hub
            var connection = new signalR.HubConnectionBuilder()
                .withUrl("/userHub")
                .build();

            // Lắng nghe sự kiện khi có User mới được tạo
            connection.on("UserCreated", function (user) {
                alert("Tài khoản mới đã được tạo: " + user.fullname);

                // Cập nhật bảng user mà không cần tải lại trang
                let userTable = document.querySelector("#userTable tbody");
                let newRow = `
                    <tr data-userid="${user.userId}" data-role="${user.roleId}">
                        <td>${user.fullname}</td>
                        <td>${user.phone}</td>
                        <td>${user.email}</td>
                        <td>${user.address || "N/A"}</td>
                        <td>${user.dateOfBirth || "N/A"}</td>
                        <td>
                            <button class="btn role-btn btn-${getRoleColor(user.roleId)}">${getRoleName(user.roleId)}</button>
                        </td>
                        <td><span class="badge badge-outline-success">Active</span></td>
                    </tr>`;
                userTable.innerHTML += newRow;
            });

            // Bắt đầu kết nối SignalR
            connection.start().catch(err => console.error(err.toString()));

            function getRoleName(roleId) {
                switch (roleId) {
                    case 1: return "User";
                    case 2: return "PT";
                    case 3: return "Admin";
                    case 4: return "Manager";
                    default: return "Unknown";
                }
            }

            function getRoleColor(roleId) {
                switch (roleId) {
                    case 1: return "primary";
                    case 2: return "success";
                    case 3: return "info";
                    case 4: return "danger";
                    default: return "secondary";
                }
            }
        });
    </script>


